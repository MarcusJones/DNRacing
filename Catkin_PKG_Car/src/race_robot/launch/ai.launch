<launch>
    
    

    
    <!-- Robot URDF Model -->
    <param name="robot_description" textfile ="$(find race_robot)urdf/race_robot.xml" />
    <arg name="model"/>
    <arg name="gui" default="false"/>

    
    <!-- Camera Node -->
    <node name="ximea" pkg="ximea_camera" type="ximea_camera_node" output="screen" >
    <param name="frame_rate" type="int" value="100" />
    <rosparam param="camera_param_file_paths" subst_value="true">[$(find ximea_camera)/config/ximea1.yaml]</rosparam>
    </node>


    <!-- Aruduino --> 
    <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
    <param name="port" value="/dev/ttyACM0"/>
    <param name="baud" value="115200"/>
    </node>
    

</launch>
