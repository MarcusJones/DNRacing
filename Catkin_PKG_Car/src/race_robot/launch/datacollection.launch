<launch>
    
    

    

    
    <!-- Camera Node -->
    <node pkg="ximea_ros_cam" type="ximea_ros_cam_node" name="cameratwo" output="screen">
        <param name="serial_no"       type="string" value="34414651" />
        <param name="cam_name"        type="string" value="ximeatwo" />
        <param name="image_directory"      type="string" value="/home/nvidia/Downloads"         />
	<param name="calib_file"      type="string" value="/home/nvidia/catkin_ws/src/ximea_ros_cam/cameratwo.yaml"         />

        <param name="frame_rate_control"        type="bool" value="true"        />
        <param name="exposure_time"        type="int" value="55000"        />
        <param name="manual_gain"        type="int" value="4"        />
        <param name="frame_rate_set"        type="int" value="5000"        />
        <param name="frame_id"        type="string" value="0"        />
        <param name="num_cams_in_bus" type="int"    value="1"        />
        <param name="bw_safetyratio"  type="double" value="1.0"      />
        <param name="publish_xi_get_image" type="bool" value="true"/>
        <param name="format" type="string" value="XI_RGB24"/>
        <param name="roi_top" type="int" value="0"/>
        <param name="roi_left" type="int" value="0"/>
        <param name="roi_width" type="int" value="1000"/>
        <param name="roi_height" type="int" value="1000"/>
        <rosparam command="load" file="$(find ximea_ros_cam)/config/ximea1.yaml" />
    </node>

    <!-- Aruduino --> 
    <node pkg="rosserial_python" type="serial_node.py" name="serial_nodetwo">
    <param name="port" value="/dev/ttyACM0"/>
    <param name="baud" value="115200"/>
    </node>
    <!-- Teleop -->
    <node pkg="Racecarteleoptwo" type= "Racecarteleop.py" name="Racecarteleoptwo">
    </node>

</launch>
